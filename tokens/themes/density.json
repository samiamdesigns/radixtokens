{
  "density": {
    "dimensions": {
      "interaction": {
        "border-width": {
          "default": {
            "type": "dimension",
            "value": "1px"
          }
        },
        "focus": {
          "border-width": {
            "type": "dimension",
            "value": "2px"
          },
          "offset": {
            "type": "dimension",
            "value": "2px"
          },
          "position": {
            "type": "dimension",
            "value": "-{density.dimensions.interaction.focus.offset}"
          }
        },
        "space": {
          "none": {
            "type": "dimension",
            "value": "0px"
          },
          "2xs": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.3xs}px"
          },
          "xs": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.2xs}px"
          },
          "sm": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.xs}px"
          },
          "md": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.sm}px"
          },
          "lg": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.md}px"
          }
        },
        "size": {
          "1": {
            "type": "dimension",
            "value": "{scale.rem.2xl}"
          },
          "2": {
            "type": "dimension",
            "value": "{scale.rem.3xl}"
          },
          "3": {
            "type": "dimension",
            "value": "{scale.rem.4xl}"
          },
          "4": {
            "type": "dimension",
            "value": "{scale.rem.5xl}"
          },
          "5": {
            "type": "dimension",
            "value": "{scale.rem.6xl}"
          },
          "touchpoint": {
            "type": "dimension",
            "value": "40px"
          },
          "05": {
            "type": "dimension",
            "value": "{dimensions.fixed.5}"
          }
        },
        "border-radius": {
          "none": {
            "type": "dimension",
            "value": "0px"
          },
          "sm": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.2xs}px"
          },
          "md": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.xs}px"
          },
          "lg": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.sm}px"
          },
          "xl": {
            "type": "dimension",
            "value": "{brand.scale-dimensions.unitless.md}px"
          },
          "full": {
            "type": "dimension",
            "value": "9999px"
          }
        },
        "control": {
          "default": {
            "size": {
              "type": "dimension",
              "value": "{brand.dimensions.control.small}"
            },
            "icon": {
              "size": {
                "type": "dimension",
                "value": "{brand.dimensions.control.small} - 0.75rem"
              }
            }
          }
        }
      }
    }
  }
}