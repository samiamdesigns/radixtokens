{
  "actions": {
    "chip": {
      "border-radius": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.border-radius.sm}"
      },
      "border-width": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.border-width.default}"
      },
      "focus": {
        "border-radius": {
          "type": "dimension",
          "value": "{actions.chip.border-radius} + {density.dimensions.interaction.focus.offset}"
        }
      },
      "input": {
        "border-width": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.border-width.default}"
        },
        "gap": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        },
        "padding-horizontal-start": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        },
        "padding-hotizontal-end": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.none}"
        },
        "padding-vertical": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.none}"
        },
        "icon": {
          "size": {
            "type": "dimension",
            "value": "16px"
          }
        }
      }
    }
  }
}