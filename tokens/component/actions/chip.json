{
  "actions": {
    "icon-button": {
      "border-radius": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.border-radius.sm}"
      },
      "border-width": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.border-width.default}"
      },
      "gap": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.space.sm}"
      },
      "height-min": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.size.touchpoint}"
      },
      "padding": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.space.sm}"
      },
      "width-min": {
        "type": "dimension",
        "value": "{density.dimensions.interaction.size.touchpoint}"
      },
      "icon": {
        "size": {
          "type": "dimension",
          "value": "16px"
        }
      },
      "focus": {
        "border-radius": {
          "type": "dimension",
          "value": "{actions.icon-button.border-radius} + {density.dimensions.interaction.focus.offset}"
        }
      },
      "contained": {
        "border-width": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.border-width.default}"
        },
        "gap": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        },
        "padding": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        },
        "icon": {
          "size": {
            "type": "dimension",
            "value": "16px"
          }
        }
      }
    }
  }
}