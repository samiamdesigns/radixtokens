{
  "actions": {
    "toggle-group": {
      "wrapper": {
        "background-color": {
          "type": "color",
          "value": "{surface.color.interactive.neutral.glass}"
        },
        "border-color": {
          "type": "color",
          "value": "{surface.color.border.neutral.default}"
        },
        "border": {
          "type": "border",
          "value": {
            "color": "{actions.toggle-group.wrapper.border-color}",
            "width": "{actions.split-button.item.border-width}",
            "style": "solid"
          }
        },
        "box-shadow": {
          "type": "boxShadow",
          "value": "{surface.elevation.interaction.none}"
        },
        "border-radius": {
          "type": "dimension",
          "value": "{actions.toggle-group.item.border-radius} + 2px"
        },
        "border-width": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.border-width.default}"
        },
        "gap": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        },
        "height-min": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.size.touchpoint}"
        },
        "padding": {
          "type": "dimension",
          "value": "{density.dimensions.interaction.space.xs}"
        }
      },
      "layout": {
        "justify-content": {
          "type": "other",
          "value": "center",
          "description": "There currently isn't a token to control this property so the value is pasted from Figma's output of CSS. Please inspect design file."
        },
        "align-items": {
          "type": "other",
          "value": "center",
          "description": "There currently isn't a token to control this property so the value is pasted from Figma's output of CSS. Please inspect design file."
        }
      }
    }
  }
