{
  "form": {
    "controls": {
      "select": {
        "item": {
          "gap": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.sm}"
          },
          "padding-horizontal": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.xs}"
          },
          "padding-vertical": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.2xs}"
          },
          "border-radius": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.border-radius.sm}"
          },
          "typography": {
            "type": "typography",
            "value": "{theme.text-style.actions.form.select-item}"
          },
          "focus": {
            "border-radius": {
              "type": "dimension",
              "value": "{form.controls.select.item.border-radius} + {density.dimensions.interaction.focus.offset}"
            }
          },
          "indicator": {
            "padding-horizontal": {
              "type": "dimension",
