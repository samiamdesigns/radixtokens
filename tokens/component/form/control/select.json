            }
          },
          "wrapper": {
            "gap": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "padding-horizontal": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "padding-vertical": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.2xs}"
            }
          }
        },
        "content": {
          "border-radius": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.border-radius.md}"
          },
          "border-width": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.border-width.default}"
          },
          "height-min": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.size.touchpoint}"
          },
          "width-min": {
            "type": "dimension",
            "value": "200px"
          },
          "padding-vertical": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.none}"
          },
          "padding-horizontal": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.none}"
          },
          "gap": {
            "type": "dimension",
            "value": "{density.dimensions.interaction.space.none}"
          },
          "background-color": {
            "type": "color",
            "value": "{surface.color.input.background.default}"
          },
          "border-color": {
            "type": "color",
            "value": "{surface.color.input.border.subtle}"
          },
          "border": {
            "type": "border",
            "value": {
              "color": "{form.controls.select.content.border-color}",
              "width": "{form.controls.select.content.border-width}",
              "style": "solid"
            }
          },
          "wrapper": {
            "padding-horizontal": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "padding-vertical": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            }
          },
          "elevation": {
            "type": "boxShadow",
            "value": "{surface.elevation.panels.small-overlay.low}"
          }
        },
        "input": {
          "content-wrapper": {
            "padding-vertical": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.none}"
            },
            "padding-start": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.sm}"
            },
            "padding-end": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "gap": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            }
          }
        }
      }
    }
  }
}