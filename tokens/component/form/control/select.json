                "value": "{surface.color.foreground.neutral.default}"
              }
            },
            "icon": {
              "color": {
                "type": "color",
                "value": "{surface.color.foreground.neutral.default}"
              }
            }
          },
          "selected": {
            "hover": {
              "bg-color": {
                "type": "color",
                "value": "{surface.color.input.background.hover}"
              },
              "label": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.hover}"
                }
              },
              "icon": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.hover}"
                }
              }
            },
            "pressed": {
              "bg-color": {
                "type": "color",
                "value": "{surface.color.input.background.selected-emphasis}"
              },
              "label": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.pressed}"
                }
              },
              "icon": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.pressed}"
                }
              }
            },
            "inactive": {
              "bg-color": {
                "type": "color",
                "value": "{surface.color.interactive.neutral.transparent}"
              },
              "label": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.inactive}"
                }
              },
              "icon": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.inactive}"
                }
              }
            },
            "disabled": {
              "bg-color": {
                "type": "color",
                "value": "{surface.color.interactive.neutral.transparent}"
              },
              "label": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.disabled}"
                }
              },
              "icon": {
                "color": {
                  "type": "color",
                  "value": "{surface.color.input.foreground.disabled}"
                }
              }
            }
          }
        },
        "group": {
          "label": {
            "typography": {
              "type": "typography",
              "value": "{theme.text-style.actions.form.select-label}"
            },
            "gap": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "padding-horizontal": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "padding-vertical": {
              "type": "dimension",
              "value": "{density.dimensions.interaction.space.xs}"
            },
            "control": {
              "padding-horizontal": {
                "type": "dimension",
                "value": "{density.dimensions.interaction.space.xs}"
              },
              "padding-vertical": {
                "type": "dimension",
                "value": "{density.dimensions.interaction.space.xs}"
              }
            },
            "bg-color": {
              "type": "color",
              "value": "{surface.color.interactive.neutral.transparent}"
            },
